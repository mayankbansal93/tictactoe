<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mayank.Tictactoe" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="my.example.mayank.Tictactoe.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="my.example.mayank.Tictactoe.SecondActivity"
            android:label="Two Players" >
        </activity>
        <activity
            android:name="my.example.mayank.Tictactoe.ThirdActivity"
            android:label="Easy Level" >
        </activity>
        <activity
            android:name="my.example.mayank.Tictactoe.FourthActivity"
            android:label="Single Player" >
        </activity>
        <activity
            android:name="my.example.mayank.Tictactoe.FifthActivity"
            android:label="Medium Level" >
        </activity>
        <activity
            android:name="my.example.mayank.Tictactoe.SixthActivity"
            android:label="Hard Level" >
        </activity>
    </application>

</manifest>
